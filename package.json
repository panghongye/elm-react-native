{
  "name": "Elm",
  "version": "0.0.1",
  "private": true,
  "author": "Lei",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "apk": "cd android && ./gradlew assembleRelease",
    "apk-i": "cd android && ./gradlew installRelease",
    "dom": "react-devtools",
    "android": "react-native run-android",
    "bundle-ios":
      "react-native bundle --entry-file index.js --platform ios     --dev false --bundle-output ./ios/bundle/main.jsbundle ",
    "bundle-android":
      "react-native bundle --entry-file index.js --platform android --dev false --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "fix":
      "rm ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-native": "^0.63.0",
    "react-native-blur": "^2.0.0",
    "react-native-parabolic": "^1.1.1",
    "react-native-scrollable-tab-view": "^0.7.1",
    "react-native-splash-screen": "^2.0.0",
    "react-native-swiper": "^1.5.4",
    "react-native-tab-navigator": "^0.3.3",
    "react-native-vector-icons": "^4.0.0"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "18.1.0",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
